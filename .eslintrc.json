{
  "extends": ["next/core-web-vitals", "next/typescript", "prettier"],
  "plugins": ["react-compiler"],
  "rules": {
    // "no-console": "error",
    "max-lines-per-function": [
      "error",
      {
        "max": 100,
        "skipBlankLines": true,
        "skipComments": true,
        "IIFEs": true
      }
    ],
    "prefer-destructuring": [
      "error",
      {
        "array": true,
        "object": true
      }
    ],
    "react-compiler/react-compiler": "error"
  }
}
